{
    "name": "pudongping/hyperf-alarm-clock",
    "description": "A utility for measuring code execution time, detecting timeouts, and sending notifications.",
    "type": "library",
    "keywords": [
        "php",
        "hyperf",
        "notifications",
        "code execution time"
    ],
    "homepage": "https://github.com/pudongping/hyperf-alarm-clock",
    "license": "MIT",
    "authors": [
        {
            "name": "pudongping",
            "email": "pudongping67@gmail.com",
            "homepage": "https://github.com/pudongping"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pudongping\\HyperfAlarmClock\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "hyperf/http-server": "~3.0.0",
        "hyperf/logger": "~3.0.0",
        "hyperf/config": "~3.0.0",
        "hyperf/guzzle": "~3.0.0"
    },
    "require-dev": {
        "swoole/ide-helper": "^5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Pudongping\\HyperfAlarmClock\\ConfigProvider"
        }
    }
}
